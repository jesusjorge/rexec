import re

def returns():
    R = U.read("https://me2.grupocva.com/me/catalogo_ip/",ignoreError=True)
    R = R.decode('iso-8859-1')
    R = R.split('dolar')[1]                                    #Text after dolar
    R = re.findall('<[^/][^>]*>\\D*(\\d+(?:\\.\\d+)?)\\D',R)   #Matches <tag>*dd(.dd)*</tag>
    R = R[0]
    return R
